@model IEnumerable<geslivraison.Models.Client>

@{
    ViewData["Title"] = "Gérer les Utilisateurs";
}

<h2>Gérer les Utilisateurs (Clients)</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Prénom</th>
            <th>Email</th>
            <th>Adresse</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model)
        {
            <tr>
                <td>@c.Nom</td>
                <td>@c.Prenom</td>
                <td>@c.Email</td>
                <td>@c.Adresse</td>
            </tr>
        }
    </tbody>
</table>
